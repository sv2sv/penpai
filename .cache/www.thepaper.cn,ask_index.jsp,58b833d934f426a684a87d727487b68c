$(document).ready(function(){
    $('#ask_ulist').infinitescroll({
        navSelector   : "#next:last",
        nextSelector  : "a#next:last",
        itemSelector  : ".topic",
        debug   : true,
        dataType    : 'html',
        loading: {
            msgText:'',
            //finishedMsg: '',
            img: "../img/loading.gif",
            selector: '#addButton'
        },
        errorCallback:function() {
            $(".askcontent").css('padding-bottom','0px');
            $("#addButton").css('margin-bottom','0px'); 
            //$("#ask_banner").css('margin-top','-20px');
        }
    });

    var pageHeight = $(".main_lt").height();
    $(".main_rt").css({minHeight: pageHeight});
});





$(".ask_sort a").hover(function(){
    $(this).addClass("on").siblings("a").removeClass("on");
},function(){
    //$(this).removeClass("on");
});

$(document).bind("mouseover",function(e){
    var target  = $(e.target);
    if(target.closest(".ask_sort").length == 0){
        $("#ontype").addClass("on").siblings("a").removeClass("on");
        $("#ontype1").addClass("on").siblings("a").removeClass("on");
    }
});

$(".ask_state a").hover(function(){
    $(this).addClass("on").siblings("a").removeClass("on");
},function(){
    //$(this).removeClass("on");
});

$(document).bind("mouseover",function(e){
    var target  = $(e.target);
    if(target.closest(".ask_state").length == 0){
        $("#onsort").addClass("on").siblings("a").removeClass("on");
    }
});


$(function(){
    var asktimeFunName = null;
	$("#askshowsearch").bind("click",function(){
		clearTimeout(asktimeFunName);
		asktimeFunName = setTimeout(function () {
			showAsksearch();
		}, 400);
	}).bind("dblclick",function(){
		clearTimeout(asktimeFunName);
		showAsksearch();
	});
})



</script>

